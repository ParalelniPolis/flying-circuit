  "version": "0.1.0",
  "description": "Website of Hackatoshi's Flying Circuit",
  "main": "dist/index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export && npm run create:cname && touch out/.nojekyll",
    "deploy": "rm -rf node_modules/.cache && npm run export && git add out/ && git commit -m \"Deploy website to gh-pages\" && git subtree push --prefix out origin gh-pages",
